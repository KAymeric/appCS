<Page x:Class="lapp_de_ses_morts.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:lapp_de_ses_morts"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Dashboard">

    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
        <StackPanel Orientation="Vertical" Width="100" Margin="0,0,700,0">
            <Menu Height="449">
                <MenuItem Header="Dashboard"/>
                <MenuItem Header="Clients"  Click="redirectClients"/>
                <MenuItem Header="Produits" />
                <MenuItem Header="Commandes" />
                <MenuItem Header="Catégorie" />
            </Menu>
        </StackPanel>
        <TextBlock Margin="100,0,-100,0" Background="White">Not Implemented</TextBlock>
    </Grid>
</Page>
